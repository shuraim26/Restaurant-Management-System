<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #FFFF;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CatDashController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="286.0" pickOnBounds="true">
         <image>
            <Image url="@cover.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="172.0" fitWidth="514.0" layoutX="286.0" pickOnBounds="true">
         <image>
            <Image url="@logo.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="del_cat" layoutX="337.0" layoutY="294.0" onAction="#del_cat" prefHeight="37.0" prefWidth="410.0" style="-fx-background-color: #D5D5D7;" text="Remove Category">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="update_cat" layoutX="391.0" layoutY="359.0" onAction="#update_cat" prefHeight="36.0" prefWidth="299.0" style="-fx-background-color: #D5D5D7;" text="Update Category">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="add_cat" layoutX="378.0" layoutY="224.0" onAction="#add_cat" prefHeight="42.0" prefWidth="330.0" style="-fx-background-color: #D5D5D7;" text="Add Category">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="logout_btn" contentDisplay="GRAPHIC_ONLY" layoutX="718.0" layoutY="147.0" prefHeight="56.0" prefWidth="69.0">
         <graphic>
            <FontAwesomeIcon glyphName="HOME" size="40" />
         </graphic>
      </JFXButton>
      <JFXButton fx:id="display_cat" layoutX="393.0" layoutY="427.0" onAction="#display_cat" prefHeight="32.0" prefWidth="299.0" style="-fx-background-color: #D5D5D7;" text="Display Categories">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
