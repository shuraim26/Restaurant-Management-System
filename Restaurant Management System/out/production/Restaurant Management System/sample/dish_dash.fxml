<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #FFFF;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DishDashController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="286.0" pickOnBounds="true">
         <image>
            <Image url="@cover.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="172.0" fitWidth="514.0" layoutX="286.0" pickOnBounds="true">
         <image>
            <Image url="@logo.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="rem_dish" layoutX="337.0" layoutY="313.0" onAction="#delete_dish" prefHeight="68.0" prefWidth="410.0" style="-fx-background-color: #D5D5D7;" text="Remove Dish">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="update_dish" layoutX="434.0" layoutY="405.0" onAction="#update_dish" prefHeight="46.0" prefWidth="227.0" style="-fx-background-color: #D5D5D7;" text="Update Dish">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="add_dish" layoutX="384.0" layoutY="216.0" onAction="#add_dish" prefHeight="68.0" prefWidth="327.0" style="-fx-background-color: #D5D5D7;" text="Add Dish">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="logout_btn" contentDisplay="GRAPHIC_ONLY" layoutX="718.0" layoutY="147.0" prefHeight="56.0" prefWidth="69.0">
         <graphic>
            <FontAwesomeIcon glyphName="HOME" size="40" />
         </graphic>
      </JFXButton>
   </children>
</AnchorPane>
