<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #FFFF;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DashboardController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="286.0" pickOnBounds="true">
         <image>
            <Image url="@cover.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="172.0" fitWidth="514.0" layoutX="286.0" pickOnBounds="true">
         <image>
            <Image url="@logo.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="billing_btn" layoutX="434.0" layoutY="182.0" prefHeight="46.0" prefWidth="227.0" style="-fx-background-color: #D5D5D7;" text="Billing">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="cat_btn" layoutX="343.0" layoutY="340.0" onAction="#cat" prefHeight="68.0" prefWidth="410.0" style="-fx-background-color: #D5D5D7;" text="Category Management">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="orders_btn" layoutX="429.0" layoutY="418.0" onAction="#orders" prefHeight="46.0" prefWidth="227.0" style="-fx-background-color: #D5D5D7;" text="Orders">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="dish_btn" layoutX="384.0" layoutY="258.0" onAction="#dish_dash" prefHeight="68.0" prefWidth="327.0" style="-fx-background-color: #D5D5D7;" text="Dish Management">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="logout_btn" contentDisplay="GRAPHIC_ONLY" layoutX="718.0" layoutY="147.0" prefHeight="56.0" prefWidth="69.0">
         <graphic>
            <FontAwesomeIcon glyphName="HOME" size="40" />
         </graphic>
      </JFXButton>
   </children>
</AnchorPane>
